{
  "schemaVersion": 1,
  "id": "pixel-addon",
  "version": "${version}",
  "name": "Pixel Addon",
  "description": "The Pixel Addon for Meteor with nice Modules and Commands.",
  "authors": [
    "pixo2000"
  ],
  "contact": {
    "repo": "https://discord.com/users/816347872181878786"
  },
  "icon": "assets/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "me.pixel.Pixel"
    ]
  },
  "mixins": [
    "pixel-addon.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
